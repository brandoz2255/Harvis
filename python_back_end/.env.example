# Harvis AI Backend Environment Variables
# Copy this file to .env and fill in your values

# Database connection string
DATABASE_URL=postgresql://user:password@host:5432/database

# Local Ollama API key (for your local/k8s Ollama instance)
OLLAMA_API_KEY=your-local-ollama-api-key

# External Ollama endpoint (for large models like qwen3:235b)
# This can be an ngrok URL, cloud endpoint, or another server running Ollama
EXTERNAL_OLLAMA_URL=https://coyotedev.ngrok.app
EXTERNAL_OLLAMA_API_KEY=your-external-ollama-api-key

# Gemini API key (optional, for Gemini model support)
GEMINI_API_KEY=your-gemini-api-key

# RAG configuration
RAG_EMBEDDING_MODEL=qwen3-embedding:4b-q4_K_M
